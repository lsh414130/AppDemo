<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.jeremylau.myapplication.activity.MainActivity">

    <Button
        android:id="@+id/test_bt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="SiderBarMenu通讯录"
        />


    <Button
        android:id="@+id/test_bt2"
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Launcher应用图标展示"
        />


    <Button
        android:id="@+id/test_bt3"
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Android js html交互"
        />

    <Button
        android:id="@+id/test_bt4"
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="动态循环加载HTML"
        />

    <Button
        android:id="@+id/test_bt5"
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="动态添加view或item并获取数据"
        />
</LinearLayout>
